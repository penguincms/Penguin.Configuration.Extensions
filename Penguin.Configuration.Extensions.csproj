<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.1;net7.0</TargetFrameworks>
    <PackageProjectUrl>https://github.com/PenguinCMS/Penguin.Configuration.Extensions</PackageProjectUrl>
    <Description>Provides extensions for Penguin configuration types to simplify data access</Description>
    <Version>5.0.5</Version>
    <AssemblyName>Penguin.Configuration.Extensions</AssemblyName>
    <RootNamespace>Penguin.Configuration.Extensions</RootNamespace>
    <DocumentationFile>Penguin.Configuration.Extensions.xml</DocumentationFile>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="7.0.0" />
    <PackageReference Include="Penguin.Configuration.Abstractions" Version="5.15.0" />
  </ItemGroup>
  <Import Project="Penguin.Projects.Shared\Penguin.Projects.Shared.csproj" />
</Project>